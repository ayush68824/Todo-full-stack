{
  "version": 2,
  "buildCommand": "cd Frontend && chmod +x build.sh && ./build.sh",
  "outputDirectory": "Frontend/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://todo-full-stack-2.onrender.com"
  }
} 